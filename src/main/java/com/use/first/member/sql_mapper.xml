<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.use.first.member.UserDAO">
	<select id="memberList" resultType="com.use.first.member.UserVO">
		select m_id, m_name, m_pw, m_gender, m_addr, m_tel, m_email, m_point, m_img, m_regdate from member
	</select>
	
	<select id="memInfo" resultType="com.use.first.member.UserVO">
		select m_id, m_name, m_pw, m_gender, m_addr, m_tel, m_email, m_point, m_img, m_regdate  from member where m_id=#{m_id}
	</select>
	
	
	
	            
    
	
